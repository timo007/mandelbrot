#!/usr/bin/env ksh

gfortran -Ofast -march=native -Wall -c mbtypes.f90
gfortran -Ofast -march=native -Wall -c mbmath.f90
gfortran -Ofast -march=native -Wall -c mrgrnk.f90
gfortran -Ofast -march=native -Wall -c setup.f90
gfortran -Ofast -march=native -Wall -c image.f90
gfortran -Ofast -march=native -Wall -c mandelbrot.f90
gfortran -Ofast -march=native -Wall -o mb mrgrnk.o setup.o mbtypes.o mbmath.o image.o mandelbrot.o
